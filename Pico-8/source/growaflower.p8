pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
function _init()
	cls()
	tic=0
end

function _update()
	tic+=1
end

function _draw()
	rectfill(0,0,127,127,1)
	rect(0,0,127,127,2)
	spr(0,48,95,4,4)

	spr(grow_stage(),48,74,4,4,flip_img())
end

function grow_stage()
	if tic < 300 then
	 return 4
	elseif tic < 600 then
		return 8
	elseif tic < 900 then
		return 12
	else
		return 64
	end
end

function flip_img()
 return flr(t()) % 2 == 0
end
