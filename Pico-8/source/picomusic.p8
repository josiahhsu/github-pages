#include shared/grid.p8
#include shared/state.p8
#include shared/math.p8:1
function _init()
	cls()
	cartdata("picomusic")
	
	// grid size
	beats = 16
	num_msrs=64
	m,n = beats*num_msrs,12
	
	function make_cell()
		//makes a cell
		local cell = {}
		cell.ins = -1
		cell.oct = -1
		cell.vol = -1
		return cell
	end
	grid=create_grid(m,n,true,
	                 make_cell)
	
	px,py,msr,ins,oct,vol,spd =
	1,1,1,0,1,1,5
	
	notes ={"b#","b","a#","a",
	        "g#","g","f#","f",
	        "e","d#","d","c#"}
	
	state = note_state()
end

function _update()
	state.controls()
	state.update()
end

function _draw()
	draw_grid()
	state.draw()
end
