function update_msr(v)
	msr = ceil(v / beats)
end

function move_horz(dx)
	if grid.in_bounds_x(px+dx) then
		px += dx
		update_msr(px)
	end
end

function move_vert(dy)
	if grid.in_bounds_y(py+dy) then
		py += dy
	end
end
