function set_state(st)
	state = st or change_state()
end

function base_state()
	// all states use x to
	// change states
	local s = template_state()
	
	s.set_btnp(❎,set_state)
	
	return s
end

function note_state()
	update_msr(px)
	local s = base_state()
	s.name="note"
	
	s.set_btnp(⬅️,move_horz,-1)
	s.set_btnp(➡️,move_horz,1)
	s.set_btnp(⬆️,move_vert,-1)
	s.set_btnp(⬇️,move_vert,1)
	s.set_btnp(🅾️,function()
		local cell = grid.get(px,py)
		if cell.ins == ins then
			cell.ins = -1
		else
			cell.ins = ins
			cell.oct = oct
			cell.vol = vol
			play_cell(px,py)
		end
	end)
	
	function s.draw()
		draw_guides({"",
		             "⬆️⬇️⬅️➡️: move cursor",
		             "🅾️: place/erase note"})
		draw_pointer()
	end
	
	return s
end

function select_state()
	update_msr(px)
	local s = base_state()
	s.name = "select"
	
	local function test_play()
		play(ins,vol,1,oct)
	end
	
	local function update_vol(dv)
		if in_range(0,3,vol+dv) then
			vol += dv
		end
		test_play()
	end
	
	s.set_btnp(⬅️,update_vol,-1)
	s.set_btnp(➡️,update_vol,1)
	
	local function update_oct(dv)
		if in_range(0,3,oct+dv) then
			oct += dv
		end
		test_play()
	end
	
	s.set_btnp(⬆️,update_oct,1)
	s.set_btnp(⬇️,update_oct,-1)
	
	s.set_btnp(🅾️,function()
		ins = (ins + 1) % 8
		test_play()
	end)
	
	function s.draw()
		draw_ins_select(9)
		draw_guides({"🅾️: cycle instrument",
		             "⬆️⬇️: change octave",
		             "⬅️➡️: change volume"})
	end
	
	return s
end

function play_state()
	local s = base_state()
	s.name="playback"
	s.t = 0
	s.beat = 1
	s.playing = false
	update_msr(s.beat)
	
	local function change_beat(db)
		if in_range(1,m,s.beat+db) then
			s.t=0
			s.beat += db
			update_msr(s.beat)
		end
	end
	
	s.set_btnp(⬅️,change_beat,-1)
	s.set_btnp(➡️,change_beat,1)
	
	local function change_spd(ds)
		if in_range(1,30,spd+ds) then
			spd += ds
		end
	end
	
	s.set_btnp(⬆️,change_spd,1)
	s.set_btnp(⬇️,change_spd,-1)
	
	s.set_btnp(🅾️,function()
		s.t = 0
		s.playing = not s.playing
	end)
	
	function s.update()
		if not s.playing then
			return
		end
		
		if s.t == 0 then
			grid.do_lane(s.beat,false,play_cell)
		elseif s.t % spd == 0 then
			if s.beat == m then
				s.playing = false
			else
				s.beat+=1
				update_msr(s.beat)
				grid.do_lane(s.beat,false,play_cell)
			end
		end
		
		s.t+=1
	end
	
	function s.draw()
		// only need the x offset
		local offset = coords(s.beat,0)
		local col = 9-tonum(s.playing)
		rect(offset,7,offset+7,7*(n+1),col)
		
		draw_guides({"🅾️: start/stop",
		             "⬅️➡️: change position",
		             "⬆️⬇️: change tempo",
		             "current speed: "..spd})
	end
	
	return s
end

function copy_state()
	local s = base_state()
	s.name="copy"
	
	local function change_msr(dm)
		if in_range(1, num_msrs,msr+dm) then
			msr += dm
		end
	end
	
	s.set_btnp(⬅️,change_msr,-1)
	s.set_btnp(➡️,change_msr,1)
	s.set_btnp(⬆️,copy_measure)
	s.set_btnp(⬇️,clear_copy)
	s.set_btnp(🅾️,paste_measure)
	
	function s.draw()
		local gd = {"⬅️➡️: change measure",
		            "⬆️: copy measure",
		            "⬇️: clear copy"}
		if dget1(0) == 1 then
			add(gd,"🅾️: paste copied measure")
		end
		draw_guides(gd)
	end
	
	return s
end

function change_state()
	local s = base_state()
	s.name="change"
	
	s.set_btn(⬅️,set_state,{note_state()})
	s.set_btn(➡️,set_state,{select_state()})
	s.set_btn(⬆️,set_state,{play_state()})
	s.set_btn(⬇️,set_state,{copy_state()})
	
	function s.draw()
		draw_guides({"⬅️: place notes",
		             "➡️: change note properties",
		             "⬆️: playback",
		             "⬇️: copy/paste measures"})
		rectfill(0,122,128,128,1)
	end
	
	return s
end
