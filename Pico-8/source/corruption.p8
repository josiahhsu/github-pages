// main menu
function title_state()
	local s={}
	s.time = 0
	s.mirror = false
	
	function s.update()
		s.time+=1
		if s.time % 36 == 0 then
			s.time = 0
			s.mirror=not s.mirror
		end
	end
	
	function s.draw()
		cls()
		map(0,0)
		local txt = {
		"‚ùé to start",
		"üÖæÔ∏è for help"
		}
		for i=1, #txt do
			print(txt[i],42,72+8*i, 7)
		end
		spr(32,64,56,1,1,s.mirror)
	end
	
	return s
end
