function _init()
	cls()
	music(0)
	
	state = title_state()
end

function _update()
	state.update()
end

function _draw()
	state.draw()
end

// corrupts the game state.
// defaults to corrupting once.
function corrupt(n)
	n = n or 1
	
	for i=1,n do
		poke(abs(rnd(-1)),rnd(-1))
	end
end
