function controls()
	//player controls
	if btn(❎) then
		if btnp(⬅️) then
			state = note_state()
		elseif btnp(➡️) then
			state = select_state()
		elseif btnp(⬆️) then
			state = play_state()
		elseif btnp(⬇️) then
			state = copy_state()
		end
		return
	end
	
	if btnp(⬅️) then
		state.left()
	elseif btnp(➡️) then
		state.right()
	elseif btnp(⬆️) then
		state.up()
	elseif btnp(⬇️) then
		state.down()
	end

	if btnp(🅾️) then
		state.o()
	end
end

function update_msr(v)
	msr = ceil(v / beats)
end

function move_horz(dx)
	if in_bounds_x(px+dx) then
		px += dx
		update_msr(px)
	end
end

function move_vert(dy)
	if in_bounds_y(py+dy) then
		py += dy
	end
end
