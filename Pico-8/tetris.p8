function make_block(x,y)
	b = {}
	b.x = x
	b.y = y
	b.spr = 3
	add(blocks, b)
	return b
end

function move_blocks(bx)
	//checks if able to
	//move left/right
	sfx(4)
	if check_piece(bx,0) then
		//moves in specified direction
		for b in all(blocks) do
			b.x += bx
		end
	end
end

function drop_blocks()
	//checks if able to move down
	if check_piece(0,1) then
		for b in all(blocks) do
			b.y += 1
		end
	else
		lock_blocks()
	end
end

// map functions account for
// positional offset
function map_get(x,y)
	return mget(x,y+1)
end

function map_set(x,y,s)
	mset(x,y+1,s)
end

function lock_blocks()
	t = 0
	//locks all blocks in place
	for b in all(blocks) do
		map_set(b.x,b.y,b.spr)
		del(blocks, b)
	end
	sfx(0)
	clear_blocks()
	piece = get_piece()
end

function is_solid(x,y)
	return fget(map_get(x,y)) == 1
end

function check_piece(bx,by)
	//returns true if pieces
	//can move
	for b in all(blocks) do
		if is_solid(b.x+bx,b.y+by) then
			return false
		end
	end
	return true
end

function clear_blocks()
	//checks if row is full
	for r = 0,14 do
		// treat as full until
		// the first empty cell
		local full = true
		for c = 3,12 do
			full = full and is_solid(c,r)
		end
		if full then
		//shift rows down
			sfx(1)
			lines += 1
			for i = r,1,-1 do
				for j = 3, 12 do
					map_set(j,i,map_get(j,i-1))
				end
			end
			//adjust interval
			//every 25 lines
			if lines % 16 == 0 and
			   interval > 3 then
				sfx(2)
				interval -= 3
				level += 1
			end
		end
	end
end
