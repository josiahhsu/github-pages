#include shared/math.p8

function _init()
	cls()
	music(0,1)
	t = 0
	blocks = {}
	lines = 0
	level = 1
	interval = 30
	bag = make_bag()
	pos = choose_piece()
	piece = get_piece()
	game_over = false
end

function _update()
	if not game_over then
		t += 1
		controls()
		if t % interval == 0 then
			drop_blocks()
		end
	end
end

function _draw()
	map(0,1,0,0,16,16)
	for b in all(blocks) do
		spr(b.spr,b.x*8,b.y*8)
	end
	draw_next()
	print('lines:',105,1,0)
	print(lines,105,9)
	print('level:',1,1)
	print(level,1,9)
end
