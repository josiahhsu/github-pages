function controls()
	if btnp(0) then //left
		move_blocks(-1)
	elseif btnp(1) then //right
		move_blocks(1)
	elseif btnp(2) then //hard drop
		hard_drop()
	elseif btn(3) then //down
		drop_blocks()
	end
	
	if btnp(4) then //c-clockwise
		rotate(.25)
	elseif btnp(5) then //clockwise
		rotate(-.25)
	end
end

function hard_drop()
	while check_piece(0,1) do
		drop_blocks()
	end
	lock_blocks()
end
