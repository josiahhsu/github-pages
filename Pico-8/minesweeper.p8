function init_state()
	// before start of game
	local s = {}

	function s.update() end

	function s.draw()
		draw_pointer()
	end

	function s.o()
		state = play_state()
		opening_move()
	end

	function s.x()
		x()
	end

	return s
end

function play_state()
	// during main gameplay
	local s = {}

	function s.update()
		t+=1/30
	end

	function s.draw()
		draw_pointer()
	end

	function s.o()
		cell_do(px,py, open_cell)
	end

	function s.x()
		x()
	end

	return s
end

function end_state()
	// game is over
	local s = {}

	function s.update() end

	function s.draw()
		draw_stats()
		if remaining == 0 then
			print("you",10,6,5)
			print("win",10,13,5)
		else
			print("game",10,6,5)
			print("over",10,13,5)
		end
		print("üÖæÔ∏è or ‚ùé",48,6,5)
		print("to restart",44,13,5)
	end

	function s.o()
		_init()
	end

	function s.x()
		_init()
	end

	return s
end
