function _init()
	cls()

	// grid size and # of mines
	m,n,mines = 18,14,40

	// cells to open
	remaining = m*n - mines

	grid = make_grid()

	// player position
	px,py = 1,1

	t = 0
	state = init_state()
end

function _update()
	controls()
	state.update()
end

function _draw()
	draw_grid()
	state.draw()
end
