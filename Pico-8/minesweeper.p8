function coords(x,y)
	//translates value to partial
	//position on grid
	return (x-1)*7,y*7+18
end

function draw_stats()
	rectfill(0,0,126,24,7)
	line(35,0,35,24,5)
	line(91,0,91,24,5)
	print("mines:"..mines,94,5,5)
	print("time:"..flr(t), 94,16,5)
end

function draw_grid()
	//draws grid and info text
	rectfill(0,0,126,128,7)
	draw_stats()
	print("‚¨ÜÔ∏è",13,4,5)
	print("‚¨ÖÔ∏è‚¨áÔ∏è‚û°Ô∏è",5,10,5)
	print("to move",3,16,5)
	print("üÖæÔ∏è to open",38,2,5)
	print("‚ùé to flag or",38,9,5)
	print("open adjacent",38,16,5)

	cell_do_all(
	function(x,y)
		//draws cells on grid
		local sx,sy = coords(x,y)
		spr(grid[x][y].spr,sx,sy)
	end
	)
end

function draw_pointer()
	//draws pointer position
	local x,y = coords(px,py)
	rect(x,y,x+7,y+7,9)
end

