function make_cell(mine)
	//makes a cell and determines
	//whether it's marked or not
	local cell = {}
	cell.spr = 20
	cell.mine = mine
	cell.flagged = false
	cell.opened = false
	return cell
end

function shuffle(t)
	for i = 1, #t do
		local j = ceil(rnd(i))
		t[i],t[j] = t[j],t[i]
	end
end

function make_grid()
	// create array of cells and
	// populate with mines
	local cells,grid = {},{}
	for i=1, m*n do
		cells[i] = make_cell(i<=mines)
	end
	shuffle(cells)

	// transform array into grid
	for i = 1, m do
		grid[i] = {}
		for j = 1, n do
			grid[i][j] = deli(cells)
		end
	end
	return grid
end
